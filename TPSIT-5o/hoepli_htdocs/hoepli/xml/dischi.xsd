<xs:element name="artista"type="xs:string"/>
<xs:element name="commento"type="xs:string"/>
<xs:element name="lunghezza"type="xs:string"/>
<xs:element name="titolo"type="xs:string"/>
<xs:element name="anno"type="xs:string"/>

<xs:elementname="canzone">
 <xs:complexType>
  <xs:sequence>
   <xs:element ref="artista"minOccurs="0"/>
   <xs:element ref="titolo"/>
   <xs:element ref="anno"minOccurs="0"/>
   <xs:element ref="commento"minOccurs="0"/>
   <xs:element ref="lunghezza"/>
  </xs:sequence>
 </xs:complexType>
</xs:element>

<xs:element name="DISCO">
 <xs:complexType>
  <xs:sequence>
   <xs:element ref="song"maxOccurs="unbounded"/>
  </xs:sequence>
 </xs:complexType>
</xs:element>

